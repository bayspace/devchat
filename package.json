		"node-fetch": "^3.3.1",
		"nonce": "^1.0.4",
		"openai": "^3.2.1",
		"quote": "^0.4.0",
		"react-infinite-scroll-component": "^6.1.0",
		"react-markdown": "^8.0.7",
		"react-syntax-highlighter": "^15.5.0",
		"shell-escape": "^0.2.0",
		"string-argv": "^0.3.2",
		"uuid": "^9.0.0"
	}
}
